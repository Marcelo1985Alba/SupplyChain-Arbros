@inherits ClientesDialogBase

<SfSpinner @ref="@refSpinner" Size="80"
           Label="Cargando..." Type="SpinnerType.Fabric">
</SfSpinner>
<SfToast ID="toast_default" @ref="ToastObj">
    <ToastPosition X="Center" Y="Top"></ToastPosition>
</SfToast>
<BuscadorEmergente Height="620px" TItem="ClienteExterno" Titulo="Listado de Clientes" Visible="@PopupBuscadorVisible"
                   DataSource="@clientes" IsModal="true" Width="90%" HeightGrid="290px"
                   Columnas="@(new string[] { "CG_CLI", "DESCRIPCION", "CUIT"})" 
                   OnObjetoSeleccionado="SendObjectSelected"
                   OnCerrarDialog="CerrarDialog"/>