                            <GridCommandColumns>
                                <GridCommandColumn Type="CommandButtonType.Edit" Title="Descargar Remito" ID="btnDescargarRemito"
                                  ButtonOption="@(new CommandButtonOptions(){IconCss="fa fa-cubes fa-1", CssClass=""})"  />
                                <GridCommandColumn Type="CommandButtonType.Edit" Title="Descargar Factura" ID="btnDescargarFactura"
