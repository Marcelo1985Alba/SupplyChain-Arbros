                                               ButtonOption="@(new CommandButtonOptions(){IconCss="fa fa-usd", CssClass=""})"></GridCommandColumn>
                            <GridCommandColumn Type="CommandButtonType.Edit" Title="Descagar Remito" ID="btnDescargarRemito"
                                               ButtonOption="@(new CommandButtonOptions(){IconCss="fa fa-cubes fa-1", CssClass=""})"></GridCommandColumn>
                            @*<GridCommandColumn Type="CommandButtonType.Edit" Title="Descagar Certificado" ID="btnDescargarCertificado"
                                               ButtonOption="@(new CommandButtonOptions(){IconCss="fa fa-download fa-1", CssClass=""})"></GridCommandColumn>
                           </GridCommandColumns>    
                        </GridColumn>*@
                        <GridColumn Field=@nameof(vESTADOS_COMPRAS.NUMERO) HeaderText="NUMERO" Width="150" Type="ColumnType.Number"></GridColumn>
                        <GridColumn Field=@nameof(vESTADOS_COMPRAS.ESTADOS_COMPRA) HeaderText="ESTADO COMPRA" Width="150"></GridColumn>
                        <GridColumn Field=@nameof(vESTADOS_COMPRAS.DES_MAT) HeaderText="DESCRIPCION" Width="150"></GridColumn>
                        <GridColumn Field=@nameof(vESTADOS_COMPRAS.DES_PROVE) HeaderText="CLIENTE" Width="150"></GridColumn>
                        <GridColumn Field=@nameof(vESTADOS_COMPRAS.PAGOS) HeaderText="PAGOS" Width="150"></GridColumn>
